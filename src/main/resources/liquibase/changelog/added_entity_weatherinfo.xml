<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property name="autoIncrement" value="true"/>
    <changeSet id="201920090004" author="hoa">
        <createTable tableName="weatherinfo">
            <column name="weather_id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="create_by" type="varchar(255)"/>
            <column name="date" type="DATETIME"/>
            <column name="description" type="varchar(255)"/>
            <column name="humidity" type="varchar(255)"/>
            <column name="icon" type="varchar(255)"/>
            <column name="id_city" type="varchar(255)"/>
            <column name="name_city" type="varchar(255)"/>
            <column name="pressure" type="varchar(255)"/>
            <column name="temp" type="INT"/>
            <column name="wind" type="varchar(255)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>